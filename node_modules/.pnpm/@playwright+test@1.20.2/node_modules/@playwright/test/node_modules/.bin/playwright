#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/agamurian/web/xyz/node_modules/.pnpm/playwright-core@1.20.2/node_modules/playwright-core/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/playwright-core@1.20.2/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules/@playwright/test/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules/@playwright/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/agamurian/web/xyz/node_modules/.pnpm/playwright-core@1.20.2/node_modules/playwright-core/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/playwright-core@1.20.2/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules/@playwright/test/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules/@playwright/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@playwright+test@1.20.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../playwright-core@1.20.2/node_modules/playwright-core/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../playwright-core@1.20.2/node_modules/playwright-core/cli.js" "$@"
fi
