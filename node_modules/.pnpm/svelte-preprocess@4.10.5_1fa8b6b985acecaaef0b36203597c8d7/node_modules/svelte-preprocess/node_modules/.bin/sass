#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/agamurian/web/xyz/node_modules/.pnpm/sass@1.50.0/node_modules/sass/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/sass@1.50.0/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/svelte-preprocess@4.10.5_1fa8b6b985acecaaef0b36203597c8d7/node_modules/svelte-preprocess/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/svelte-preprocess@4.10.5_1fa8b6b985acecaaef0b36203597c8d7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/agamurian/web/xyz/node_modules/.pnpm/sass@1.50.0/node_modules/sass/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/sass@1.50.0/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/svelte-preprocess@4.10.5_1fa8b6b985acecaaef0b36203597c8d7/node_modules/svelte-preprocess/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/svelte-preprocess@4.10.5_1fa8b6b985acecaaef0b36203597c8d7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.50.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.50.0/node_modules/sass/sass.js" "$@"
fi
