#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules/vite/bin/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules/vite/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules/vite/bin/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules/vite/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/vite@2.9.1_sass@1.50.0/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.9.1_sass@1.50.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.9.1_sass@1.50.0/node_modules/vite/bin/vite.js" "$@"
fi
