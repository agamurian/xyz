#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules/esbuild/bin/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules/esbuild/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules/esbuild/bin/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules/esbuild/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/esbuild@0.14.34/node_modules:/home/agamurian/web/xyz/node_modules/.pnpm/node_modules:/home/agamurian/web/xyz/node_modules:/home/agamurian/web/node_modules:/home/agamurian/node_modules:/home/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
